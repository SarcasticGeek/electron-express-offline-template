{
  "name": "electron-express-offline-template",
  "version": "1.0.0",
  "description": "Offline App template with electron ,Express ,and nedb",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager .  --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win-64": "electron-packager . electron_express_offline_template --overwrite --asar=false --platform=win32 --arch=x64  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"electron express offline template\"",
    "package-win-32": "electron-packager . electron_express_offline_template --overwrite --asar=false --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"electron express offline template\"",
    "package-linux": "electron-packager . electron_express_offline_template --overwrite --asar=false --platform=linux --arch=x64  --prune=true --out=release-builds",
    "windows-installer": "node ./build-installer.js"
  },
  "repository": "https://github.com/SarcasticGeek/electron-express-offline-template",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "SarcasticGeek",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-installer-windows": "^1.1.0",
    "electron-packager": "^12.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.6.4",
    "pkg": "^4.3.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "jquery": "^3.3.1",
    "shelljs": "^0.8.2"
  }
}
